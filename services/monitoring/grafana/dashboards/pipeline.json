{
  "title": "ASR-LLM-TTS Pipeline",
  "uid": "pipeline-overview",
  "schemaVersion": 39,
  "timezone": "browser",
  "refresh": "5s",
  "time": { "from": "now-15m", "to": "now" },
  "panels": [
    {
      "id": 1,
      "title": "Active Calls",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "pipeline_calls_active", "legendFormat": "active" }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [
          { "color": "green", "value": null },
          { "color": "yellow", "value": 50 },
          { "color": "red", "value": 90 }
        ]}}
      }
    },
    {
      "id": 2,
      "title": "Total Calls",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "pipeline_calls_total", "legendFormat": "total" }
      ]
    },
    {
      "id": 3,
      "title": "Calls/sec",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "rate(pipeline_calls_total[1m])", "legendFormat": "calls/s" }
      ]
    },
    {
      "id": 4,
      "title": "Errors",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "sum(rate(pipeline_errors_total[1m]))", "legendFormat": "errors/s" }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [
          { "color": "green", "value": null },
          { "color": "red", "value": 1 }
        ]}}
      }
    },
    {
      "id": 5,
      "title": "Stage Latency (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.95, rate(pipeline_stage_duration_seconds_bucket{stage=\"asr\"}[1m]))", "legendFormat": "ASR p95" },
        { "refId": "B", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.95, rate(pipeline_stage_duration_seconds_bucket{stage=\"llm\"}[1m]))", "legendFormat": "LLM p95" },
        { "refId": "C", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.95, rate(pipeline_stage_duration_seconds_bucket{stage=\"tts\"}[1m]))", "legendFormat": "TTS p95" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 6,
      "title": "End-to-End Latency",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.50, rate(pipeline_e2e_duration_seconds_bucket[1m]))", "legendFormat": "p50" },
        { "refId": "B", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.95, rate(pipeline_e2e_duration_seconds_bucket[1m]))", "legendFormat": "p95" },
        { "refId": "C", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.99, rate(pipeline_e2e_duration_seconds_bucket[1m]))", "legendFormat": "p99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 7,
      "title": "Audio Chunks/sec",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 12 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "rate(audio_chunks_processed_total[1m])", "legendFormat": "chunks/s" }
      ]
    },
    {
      "id": 8,
      "title": "Speech Segments/sec",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 12 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "rate(vad_speech_segments_total[1m])", "legendFormat": "segments/s" }
      ]
    },
    {
      "id": 9,
      "title": "Embedding Latency (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 18 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.95, rate(pipeline_embedding_duration_seconds_bucket[1m]))", "legendFormat": "embed p95" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 10,
      "title": "RAG Retrieval Latency (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 18 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "histogram_quantile(0.95, rate(pipeline_rag_duration_seconds_bucket[1m]))", "legendFormat": "RAG p95" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ]
}
